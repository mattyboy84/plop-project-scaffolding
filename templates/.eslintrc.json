{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "linebreak-style": "off",
        "no-param-reassign": "off",
        "spaced-comment": "off",
        "no-await-in-loop": "off"
    }
}
